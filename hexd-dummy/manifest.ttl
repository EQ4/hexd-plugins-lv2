@prefix lv2: <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

<http://github.com/danilobellini/hexd-plugins-lv2/hexd-dummy>
  a lv2:Plugin, lv2:UtilityPlugin;

  lv2:binary <hexd-dummy.so>;

  lv2:port [
    a lv2:AudioPort, lv2:InputPort;
    lv2:name "Input";
    lv2:index 0;
    lv2:symbol "in";
  ], [
    a lv2:AudioPort, lv2:OutputPort;
    lv2:name "Output";
    lv2:index 1;
    lv2:symbol "out";
  ];

  doap:name "hexD Dummy";

  doap:developer [
    foaf:name "Danilo de Jesus da Silva Bellini";
    foaf:homepage <http://github.com/danilobellini>;
    foaf:mbox <mailto:danilo.bellini@gmail.com>;
  ];

  doap:maintainer [
    foaf:name "Danilo de Jesus da Silva Bellini";
    foaf:homepage <http://github.com/danilobellini>;
    foaf:mbox <mailto:danilo.bellini@gmail.com>;
  ];

  doap:license <GPLv3>;

  rdfs:comment """
A dummy plugin that just copy the input to the output.
""".
